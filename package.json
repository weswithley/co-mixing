    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "gsap": "^3.11.1",
    "npm": "^8.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.2.5",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.1",
