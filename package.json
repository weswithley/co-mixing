  "outputConfig": {
    "dev": {
      "outputPath": "bundle"
    },
    "prod": {
      "outputPath": "bundle",
      "filename": "bundle.[name].[contenthash].js"
    }
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
